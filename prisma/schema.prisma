generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model other_test {
  id      Int     @id @default(autoincrement())
  name    String?
  message String?
}

model test {
  id      Int     @id @default(autoincrement())
  name    String?
  message String?
}
